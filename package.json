    "@fortawesome/free-solid-svg-icons": "6.4.2",
    "bulma-switch": "^2.0.4",
    "ics": "^3.5.0",
    "luxon": "^3.4.3",
    "svelte-fa": "^3.0.4",
    "svelte-markdown": "^0.4.0",
