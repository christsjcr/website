{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"resolveJsonModule": true,
		"allowSyntheticDefaultImports": true,
		"moduleResolution": "bundler",
		"module": "ESNext",
		"target": "ES2018"
	},
	"include": [
		"src/**/*.d.ts",
		"src/**/*.js",
		"src/**/*.ts",
		"src/**/*.svelte"
	]
}